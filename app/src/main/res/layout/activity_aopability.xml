<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context="com.hujiang.library.demo.AOPAbilityActivity">
        <Button
            android:id="@+id/id_execution"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="execution"/>
        <Button
            android:id="@+id/id_call"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="call"/>
        <Button
            android:id="@+id/id_args"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="args"/>
        <Button
            android:id="@+id/id_annotation"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="annotation"/>
        <Button
            android:id="@+id/id_within"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="within"/>
        <Button
            android:id="@+id/id_not_within"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="!within"/>
        <Button
            android:id="@+id/star_match"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="* 匹配"/>
        <Button
            android:id="@+id/double_dot_match"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=".. 匹配"/>
        <Button
            android:id="@+id/plus_match"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="+ 匹配"/>
        <Button

            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </LinearLayout>
</ScrollView>

